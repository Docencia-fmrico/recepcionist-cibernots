<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Repeat num_cycles="1">
            <ReactiveFallback>
                <Condition ID="FindChair"/>
                <Action ID="SearchChair"/>
            </ReactiveFallback>
        </Repeat>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Condition ID="FindChair"/>
        <Action ID="GoChair"/>
        <Action ID="ReachedChair"/>
        <Action ID="SearchChair"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
