<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Repeat num_cycles="2">
            <Sequence>
                <Action ID="GoTo" Point="{Door}"/>
                <Action ID="GoTo" Point="{Party}"/>
                <Action ID="GoTo" Point="{chair1}"/>
                <Action ID="GoTo" Point="{chair2}"/>
                <Action ID="GoTo" Point="{chair3}"/>
                <Action ID="GoTo" Point="{chair4}"/>
                <Action ID="GoTo" Point="{Barman}"/>
            </Sequence>
        </Repeat>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="GoTo">
            <input_port name="Point"/>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
